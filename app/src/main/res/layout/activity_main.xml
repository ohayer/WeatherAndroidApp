<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".running.MainActivity" android:id="@+id/city_autocomplete">
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hello World!"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:id="@+id/helloWorld"/>
    <EditText
            android:layout_width="211dp"
            android:layout_height="61dp"
            android:inputType="textPersonName"
            android:text="Warsaw"
            android:imeOptions="actionDone"
            android:ems="10"
            android:id="@+id/editTextTextCityName"
            tools:ignore="MissingConstraints"
            app:layout_constraintBottom_toTopOf="@+id/helloWorld" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>